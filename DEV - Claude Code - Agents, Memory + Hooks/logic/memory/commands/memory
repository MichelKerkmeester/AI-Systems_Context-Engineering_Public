#!/usr/bin/env python3
# Wrapper script for memory command
import os
import sys

# Get the directory of this script
script_dir = os.path.dirname(os.path.abspath(__file__))
command_script = os.path.join(script_dir, 'memory-command.py')

# Execute the command script with all arguments
os.execv(sys.executable, [sys.executable, command_script] + sys.argv[1:])