{
  "orchestrator": {
    "max_agents": 5,
    "auto_synthesis": true,
    "synthesis_threshold": 3,
    "worktree_enabled": true,
    "message_queue_size": 1000,
    "agent_pools": {
      "analyst": {
        "min": 1,
        "max": 2,
        "preferred_model": "gemini-mcp"
      },
      "developer": {
        "min": 2,
        "max": 3,
        "preferred_model": "kimi-pro"
      },
      "reviewer": {
        "min": 1,
        "max": 2,
        "preferred_model": "kimi-pro"
      },
      "synthesis": {
        "min": 0,
        "max": 1,
        "preferred_model": "claude-opus-4"
      }
    }
  },
  "work_distribution": {
    "strategy": "complexity_first",
    "load_balancing": true,
    "prefer_specialization": true,
    "max_retries": 3
  },
  "performance": {
    "parallel_limit": 5,
    "task_timeout": 300,
    "health_check_interval": 30,
    "auto_scale": true
  },
  "synthesis": {
    "auto_trigger": true,
    "min_agents_for_synthesis": 2,
    "min_packages_for_synthesis": 3,
    "conflict_resolution": "expert_judgment",
    "always_synthesize_on_completion": true
  },
  "worktree": {
    "enabled": true,
    "auto_cleanup": true,
    "branch_prefix": "agent",
    "isolate_developers": true,
    "merge_strategy": "rebase"
  },
  "monitoring": {
    "log_level": "INFO",
    "metrics_enabled": true,
    "performance_tracking": true,
    "cost_tracking": true
  },
  "model_routing": {
    "enable_intelligent_routing": true,
    "cost_optimization_priority": 0.8,
    "quality_threshold": 0.7,
    "fallback_model": "claude-opus-4"
  }
}