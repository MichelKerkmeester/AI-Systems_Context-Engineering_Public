{
  "enabled": true,
  "auto_archive": true,
  "auto_create": true,
  "session_limit": 10,
  "backup_before_delete": true,
  "backup_limit": 5,
  "timestamp_format": "%Y-%m-%d-%H%M%S",
  "default_description": "auto-session",
  "prompt_for_description": false,
  "trigger_on_first_command": true,
  "exclude_tools": ["Read", "LS", "Grep", "Glob"],
  "session_timeout_hours": 4,
  "description_extraction": {
    "enabled": true,
    "patterns": {
      "git": {
        "commit": "git-commit",
        "push": "git-push", 
        "pull": "git-pull",
        "default": "git-work"
      },
      "npm": "package-mgmt",
      "test": "testing",
      "build": "building",
      "fix": "bug-fix",
      "feature": "feature-dev",
      "refactor": "refactoring"
    }
  },
  "feedback": {
    "show_creation": true,
    "show_existing": false,
    "show_errors": true
  }
}